{
  "executionRoleArn": "arn:aws:iam::495599755529:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "Container-app-flask",
          "image": "<IMAGE1_NAME>",
          "essential": true,
          "portMappings": [
              {
                  "containerPort": 5000,
                  "hostPort": 5000,
                  "protocol": "tcp"
              }
          ]
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024",
  "family": "Task-app-flask"
}